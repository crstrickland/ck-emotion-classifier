    "import os\n",
    "\n",
    "emotions = []\n",
    "for path, name, files in os.walk('Emotion'):\n",
    "    for file in files:\n",
    "        if(file):\n",
    "            with open(os.path.join(path, file)) as curf:\n",
    "                emotions.append((file[:-4], curf.read().strip(' ')[:1]))\n",
    "\n",
    "print(emotions[0])                "
   ]
